package toucan.modele;

public class MouvementHaut extends Mouvement{
	
	public MouvementHaut (int d) {
		this.distance = d;
		this.yarr = this.y - d;
		
	}


	public MouvementHaut(Mouvement mouvement, int d) {
		super( mouvement, d);
		this.yarr = this.y - d;
	}


	@Override
	public int posX(int t) {
		return xarr;
	}

	@Override
	public int posY(int t) {
		return y - ( t - y);
	}

}
