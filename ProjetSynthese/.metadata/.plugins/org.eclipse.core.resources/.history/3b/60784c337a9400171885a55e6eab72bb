package toucan.modele;

public class LesMouvements {
	Mouvement[] lesMouvs = new Mouvement[4];
	protected int xInit;
	protected int yInit;
	
	public LesMouvements(int x, int y , int t){
		Mouvement m = new Mouvement(t, x, y);
	}
	
	public int posX(int t){
		return this.xInit + t;
	}
	
	public int posY(int t){
		return this.yInit + t;
	}
}
