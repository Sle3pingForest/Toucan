package toucan.modele;

public class Mouvement {
	protected int duree;
	protected int xDepart;
	protected int yDepart;
	
	public Mouvement(int time, int x, int y){
		this.duree = time;
		this.xDepart = x;
		this.yDepart = y;
	}

	public int getX(){
		return this.xDepart;
	}
	
	public int getY(){
		return this.yDepart;
	}
	
}
